{
    "name": "rhymedigital/contao_backend_rhyme",
    "type": "contao-bundle",
    "description": "A Contao Open Source CMS Backend Theme",
    "license": "LGPL-3.0-or-later",
    "keywords": [
        "contao",
        "theme"
    ],
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "https://github.com/rhymedigital"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Rhyme\\ContaoBackendThemeBundle\\": "src"
        },
        "exclude-from-classmap": [
            "src/Resources/contao/themes/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Rhyme\\ContaoBackendThemeBundle\\ContaoManager\\Plugin"
    }
}